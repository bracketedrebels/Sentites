{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "stripInternal": true,
        "preserveConstEnums": false,
        "declaration": true,
        "noResolve": false
    }
}
